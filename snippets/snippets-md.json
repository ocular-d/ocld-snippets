{
    "Codeblock Shell":{
        "scope": "markdown",
        "prefix": "ocld-CodeBlockShell",
        "body": [
            "```shell",
            "",
            "```"
        ],
        "description": "Insert Shell Codeblock."
    },
    "Codeblock JSON":{
        "scope": "markdown",
        "prefix": "ocld-CodeBlockJSON",
        "body": [
            "```json",
            "",
            "```"
        ],
        "description": "Insert JSON Codeblock."
    },
    "Codeblock Python":{
        "scope": "markdown",
        "prefix": "ocld-CodeBlockPython",
        "body": [
            "```python",
            "",
            "```"
        ],
        "description": "Insert Python Codeblock."
    },
    "List":{
        "scope": "markdown",
        "prefix": "ocld-List",
        "body": [
            "- ${1:Item-1}",
            "- ${2:Item-2}"
        ],
        "description": "Insert A List"
    },
    "Link":{
        "scope": "markdown",
        "prefix": "ocld-Link",
        "body": [
            "[${1:Link-Name}(${2:URL}]"
        ],
        "description": "Insert A Link"
    },
    "CheckBox Unchecked": {
        "prefix": "ocld-Box",
        "body": [
            "[ ] $1"
        ],
        "description": "Insert An Unchecked Checkbox."
    },
    "Inline code": {
        "prefix": "ocld-InlineCode",
        "body": [
            "`${1:YourInlineCodeHere}`"
        ],
        "description": "Insert An Inline Code."
    },
    "Bold": {
        "prefix": "ocld-Bold",
        "body": [
            "**${1:Text}**"
        ],
        "description": "Adds A Bold Text."
    },
    "Italic": {
        "prefix": "ocld-Italic",
        "body": [
            "*${1:Text}*"
        ],
        "description": "Adds An Italic Text."
    },
    "Image": {
        "prefix": "ocld-Image",
        "body": [
            "![${1:Alt-Text}](${2:Path/URL})"
        ],
        "description": "Adds An Image."
    }
}