{
    "name": "ocld-snippets",
    "displayName": "ocld-snippets",
    "publisher": "ocld",
    "description": "ocular-d snippets for (technical) writing",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ocular-d/ocld-snippets"
    },
    "icon": "icon.png",
    "contributes": {
        "snippets": [
            {
                "language": "markdown",
                "path": "./snippets/snippets-md.json"
            },
            {
                "language": "restructuredtext",
                "path": "./snippets/snippets-sphinx.json"
            }
        ]
    },
    "devDependencies": {
        "vuepress": "^1.4.0"
    },
    "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  }
}
